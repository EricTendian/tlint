{
    "name": "tightenco/tlint",
    "version": "2.1.1",
    "description": "Tighten linter for Laravel conventions",
    "require": {
        "php": ">=7.1",
        "nikic/php-parser": "^4.1",
        "symfony/console": "^4.1",
        "illuminate/view": "^5.7",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Tighten\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "bin": ["bin/tlint"],
    "scripts": {
        "runTests": "./scripts/runTests.sh",
        "buildTestEnvironments": "./scripts/buildTestEnvironments.sh"
    },
    "license": "MIT"
}
