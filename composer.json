{
    "name": "tightenco/tlint",
    "version": "3.0.4",
    "description": "Tighten linter for Laravel conventions",
    "require": {
        "php": ">=7.2",
        "nikic/php-parser": "^4.2",
        "symfony/console": "^4.3",
        "illuminate/view": "^6.0",
        "symfony/process": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Tighten\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "bin": ["bin/tlint"],
    "scripts": {
        "runTests": "./scripts/runTests.sh",
        "buildTestEnvironments": "./scripts/buildTestEnvironments.sh"
    },
    "license": "MIT"
}
